# Autodetect text files
* text=auto

# Force the following filetypes to have unix eols, so Windows does not break them
*.* text eol=lf

# Force images/fonts to be handled as binaries
*.jpg binary
*.jpeg binary
*.gif binary
*.png binary
*.ico binary
*.webm binary
*.webp binary
*.exe binary
*.data binary
*.ttf binary
*.eof binary
*.eot binary
*.otf binary
*.woff binary
*.woff2 binary
